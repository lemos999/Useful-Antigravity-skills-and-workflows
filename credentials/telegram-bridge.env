# Telegram Bridge Credentials
# DO NOT COMMIT THIS FILE TO VERSION CONTROL

# Telegram Bot Token (from @BotFather)
# 1. Open Telegram and search for @BotFather
# 2. Send /newbot command
# 3. Follow prompts to name your bot
# 4. Copy the API token below
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN_HERE

# Telegram User ID (allowed to use the bot)
# ID of the Telegram user who is authorized to chat with this bot
TELEGRAM_USER_ID=YOUR_TELEGRAM_USER_ID_HERE

# OpenClaw Gateway Token (auto-generated during onboard)
OPENCLAW_GATEWAY_TOKEN=YOUR_OPENCLAW_GATEWAY_TOKEN_HERE

# Proxy Settings
PROXY_BASE_URL=http://127.0.0.1:8080
PROXY_API_KEY=test
